{
  "name": "eaglelogger",
  "version": "0.0.1",
  "description": "",
  "main": "webpack.config.babel.js",
  "keywords": [],
  "author": "Kevin Ashcraft <kevin@kevashcraft.com> (https://www.kevashcraft.com)",
  "license": "ISC",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "api:build": "rimraf ./dist/api/ && babel ./src/api/ --out-dir ./dist/api/ --copy-files --source-maps inline",
    "api:dev": "npm run api:build && node ./dist/api/api.js",
    "site:build": "webpack --progress --colors",
    "site:dev": "webpack-dev-server --content-base ./src/site/static --content-base generated --inline --hot --history-api-fallback --host 0.0.0.0 --colors",
    "build": "npm run api:build && npm run site:build",
    "dev": "npm run site:dev | npm-watch api:dev"
  },
  "watch": {
    "api:dev": {
      "patterns": [
        "src/api/**/*.js"
      ]
    }
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "copy-webpack-plugin": "^4.2.3",
    "css-loader": "^0.28.7",
    "csv-parse": "^2.0.0",
    "csv-parser": "^1.12.0",
    "file-loader": "^1.1.5",
    "html-webpack-plugin": "^2.30.1",
    "material-design-icons": "^3.0.1",
    "mdi": "^2.0.46",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "npm-watch": "^0.3.0",
    "path": "^0.12.7",
    "pg": "^7.4.0",
    "pg-native": "^2.2.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "sass-loader": "^6.0.6",
    "socket.io": "^2.0.4",
    "socket.io-file": "^2.0.2",
    "socket.io-file-client": "^2.0.2",
    "style-loader": "^0.19.0",
    "url-loader": "^0.6.2",
    "vue": "^2.5.9",
    "vue-loader": "^13.5.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.9",
    "vuetify": "^0.17.4",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.4.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7"
  }
}
