{
  "name": "eaglelogger",
  "version": "0.0.1",
  "description": "",
  "main": "webpack.config.babel.js",
  "keywords": [],
  "author": "Kevin Ashcraft <kevin@kevashcraft.com> (https://www.kevashcraft.com)",
  "license": "ISC",
  "scripts": {
    "test": "babel-node -- ./node_modules/karma/bin/karma start",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "api:build": "rimraf ./dist/api/ && babel ./src/api/ --out-dir ./dist/api/ --copy-files --source-maps inline && babel ./src/components/ --out-dir ./dist/components/ --copy-files --source-maps inline",
    "api:dev": "npm run api:build && node ./dist/api/api.js",
    "db:build": "devdata=true force=true ./src/api/db/init && echo 'BUILDING'",
    "site:build": "webpack --progress --colors",
    "site:dev": "webpack-dev-server --content-base ./src/site/static --content-base generated --inline --hot --history-api-fallback --host 0.0.0.0 --colors",
    "build": "npm run api:build && npm run site:build",
    "dev": "npm run site:dev | npm-watch"
  },
  "watch": {
    "api:dev": {
      "patterns": [
        "src/**/*.js"
      ]
    },
    "db:build": {
      "patterns": [
        "src"
      ],
      "extensions": "sql"
    }
  },
  "dependencies": {
    "avoriaz": "^6.3.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-3": "^6.24.1",
    "chai": "^4.1.2",
    "copy-webpack-plugin": "^4.3.1",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.8",
    "csv-parse": "^2.0.0",
    "csv-parser": "^1.12.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "karma": "^2.0.0",
    "karma-babel-preprocessor": "^7.0.0",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^2.2.0",
    "karma-coverage": "^1.1.1",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-mocha": "^1.3.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^2.0.9",
    "material-design-icons": "^3.0.1",
    "mdi": "^2.1.19",
    "mocha": "^4.1.0",
    "moment": "^2.20.1",
    "node-sass": "^4.7.2",
    "npm-watch": "^0.3.0",
    "path": "^0.12.7",
    "pg": "^7.4.1",
    "pg-native": "^2.2.0",
    "request": "^2.83.0",
    "request-promise-native": "^1.0.5",
    "sass-loader": "^6.0.6",
    "socket.io": "^2.0.4",
    "socket.io-file": "^2.0.15",
    "socket.io-file-client": "^2.0.121",
    "style-loader": "^0.19.1",
    "uglifyjs-webpack-plugin": "^1.1.6",
    "url-loader": "^0.6.2",
    "vue": "^2.5.13",
    "vue-loader": "^13.7.0",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.5.13",
    "vuetify": "^0.17.6",
    "vuex": "^3.0.1",
    "vuex-persistedstate": "^2.4.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.10.1"
  }
}
