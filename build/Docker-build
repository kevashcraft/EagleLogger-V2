#!/bin/bash

docker build -t kevashcraft/eaglelogger-postgres:latest -f build/Dockerfile-postgres .
docker build -t kevashcraft/eaglelogger-site:latest -f build/Dockerfile-site .
docker build -t kevashcraft/eaglelogger-api:latest -f build/Dockerfile-api .
docker build -t kevashcraft/eaglelogger-cron:latest -f build/Dockerfile-cron .

docker push kevashcraft/eaglelogger-postgres:latest
docker push kevashcraft/eaglelogger-site:latest
docker push kevashcraft/eaglelogger-api:latest
docker push kevashcraft/eaglelogger-cron:latest
